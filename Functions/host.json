{
    "version": "2.0",
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[1.*, 2.0.0)"
    },
    "extensions": {
      "http": {
        "routePrefix": "api"
      }
    },
    "tracing": {
      "consoleLevel": "verbose"
    },
    "logging": {
      "logLevel": {
        "default": "Information"
      }
    },
    "httpWorker": {
      "description": {
        "defaultExecutablePath": "dotnet",
        "defaultWorkerPath": "FunctionCustomHandlers.dll"
      }
    }
  }